{
    "env": {
        "browser": true,
        "es6":true,
        "node":true,
         "jest": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },

     "globals": {
          "Atomics":"readonly",
           "SharedArrayBuffer":"readonly"

        },
    "plugins": [
        "react"
    ],
    "rules": {
       "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
         "linebreak-style": 0,
          "react/prop-types": 0,
          "react/forbid-prop-types": 0

    }
}
