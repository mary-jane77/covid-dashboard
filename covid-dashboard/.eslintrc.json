{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "rules": {
      // "no-shadow": "off",
      // "no-param-reassign": [2, { "props": false }],
      "no-plusplus": "off",
      "eqeqeq": "off",
      "import/prefer-default-export": "off",
      "no-console": "off"
  }

}